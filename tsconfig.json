{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"jsx": "preserve",
		"lib": [
			"dom",
			"es2017"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"removeComments": false,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"paths": {
			"@Components/*": [
				"./src/Components/*"
			],
			"@Components": [
				"./src/Components"
			],
			"@Interfaces": [
				"./src/Interfaces"
			],
			"@Interfaces/*": [
				"./src/Interfaces/*"
			],
			"@Definitions": [
				"./src/Definitions"
			],
			"@Types": [
				"./src/Types"
			],
			"@Reducers": [
				"./src/Redux/Reducers"
			],
			"@Reducers/*": [
				"./src/Redux/Reducers/*"
			],
			"@Redux": [
				"./src/Redux"
			],
			"@Redux/*": [
				"./src/Redux/*"
			],
			"@Actions": [
				"./src/Actions"
			],
			"@Styles": [
				"./src/Styles"
			],
			"@Styles/*": [
				"./src/Styles/*"
			]
		}
	}
}